@model IEnumerable<TallerMotos.Models.ViewData.Top10Empleados>
@{ Layout = "~/Views/Shared/_Layout2.cshtml";

    
}


<h1>A estos tios les vamos a subir el sueldo</h1>
<br />

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>
                Empleado
            </th>
            <th>
                Ventas
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombreEmpleado)
                @Html.DisplayFor(modelItem => item.apellidoEmpleado)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.t)€
            </td>

        </tr>
        }
    </tbody>
</table>
<a asp-action="Index" asp-controller="Empleados" style="color:white">Volver a Empleados</a>



<div class="row">

    <div id="container" class="col-6"></div>

    <div id="container2" class="col-6"></div>
</div>
