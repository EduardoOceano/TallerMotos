    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Core theme JS-->
    <<script type="text/javascript">
        $(document).ready(function (ev) {
            $("#galeria li:gt(0)").hide(); //gt(0) se refiere a todos menos al elemento cuyo indice sea 0

            setInterval(function () { //se ejecuta el intervalo,
                $("#galeria li:first-child").fadeOut()
                    .next("li").fadeIn(1000)
                    .end().appendTo("#galeria");
            }, 4000); //numero de milisegundos


        });

    </script>


